<!DOCTYPE html>
<html>
    <head>
    	<title>demo1</title>
    	<meta charset="UTF-8">
    </head>
    <body>
   </body> 
    <script type="text/javascript" >
      document.write("<table border='1' width='800'>");
        for(var i=0;i<11;i++)
        {
              if (i%2 != 0)
              {
                document.write("<tr height='20px' bgcolor='#ADADAD'>");
              }
              else
              {
                document.write("<tr height='20px'>");
              }
              for(var j=0;j<8;j++)
              {
                document.write("<td>&nbsp;</td>");
              }
        }
      document.write("</table>");
      </script>
</html>
